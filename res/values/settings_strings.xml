<resources>
	<string name="SETTING_LABEL_ALLOW_READ_DEVICE_ID">Device ID</string>
	<string name="SETTING_HELP_ALLOW_READ_DEVICE_ID">Controls access to the unique and permanent ID for the device: MEID or ESN for CDMA devices, IMEI for GSM devices.</string>
	<string name="SETTING_LABEL_ALLOW_READ_PHONE_NUMBER">Phone Number</string>
	<string name="SETTING_HELP_ALLOW_READ_PHONE_NUMBER">Controls access to the phone number for the device from the SIM (which is often absent or invalid).</string>
	<string name="SETTING_LABEL_ALLOW_READ_SIM_SERIAL">Sim Card Serial</string>
	<string name="SETTING_HELP_ALLOW_READ_SIM_SERIAL">Controls access to the serial number (ICCID) for the SIM in use. This is unique to the SIM.</string>
	<string name="SETTING_LABEL_ALLOW_READ_SUBSCRIBER_ID">Subscriber ID</string>
	<string name="SETTING_HELP_ALLOW_READ_SUBSCRIBER_ID">Controls access of the subscriber ID (or IMSI) from the SIM (or R-UIM in CDMA) in use. This reveals the country, mobile network, and an ID unique to the SIM or R-UIM.</string>
	<string name="SETTING_LABEL_ALLOW_READ_INCOMING_CALL_NUMBER">Incoming Call Number</string>
	<string name="SETTING_HELP_ALLOW_READ_INCOMING_CALL_NUMBER">Controls access of the calling number for incoming phone calls.</string>
	<string name="SETTING_LABEL_ALLOW_READ_OUTGOING_CALL_NUMBER">Outgoing Call Number</string>
	<string name="SETTING_HELP_ALLOW_READ_OUTGOING_CALL_NUMBER">Controls access of the destination number for outgoing phone call.</string>
	<string name="SETTING_LABEL_ALLOW_INITIATE_PHONE_CALL">Call Phone</string>
	<string name="SETTING_HELP_ALLOW_INITIATE_PHONE_CALL">Controls whether an app can make outgoing phone calls.</string>
	<string name="SETTING_LABEL_ALLOW_READ_NETWORK_LOCATION">Network Location</string>
	<string name="SETTING_HELP_ALLOW_READ_NETWORK_LOCATION">Controls access to your location, as determined by cell towers and Wifi networks</string>
	<string name="SETTING_LABEL_ALLOW_READ_GPS_LOCATION">GPS Location</string>
	<string name="SETTING_HELP_ALLOW_READ_GPS_LOCATION">Controls access to your location, as determined by GPS</string>
	<string name="SETTING_LABEL_ALLOW_USE_ACCOUNT_CREDENTIALS">Account Credentials</string>
	<string name="SETTING_HELP_ALLOW_USE_ACCOUNT_CREDENTIALS">Controls whether an app can use an account associated with that app for authentication with a service (e.g. the Facebook app using your Facebook credentials)</string>
	<string name="SETTING_LABEL_ALLOW_READ_ACCOUNTS_LIST">Accounts</string>
	<string name="SETTING_HELP_ALLOW_READ_ACCOUNTS_LIST">Controls whether the app can obtain a list of accounts on the device, including the ID associated with the account (e.g. your Gmail address, your Facebook ID, etc).</string>
	<string name="SETTING_LABEL_ALLOW_READ_CONTACTS">Contacts</string>
	<string name="SETTING_HELP_ALLOW_READ_CONTACTS">Controls access to the full details of contacts in your address book.</string>
	<string name="SETTING_LABEL_ALLOW_READ_CALL_LOG">Call Log</string>
	<string name="SETTING_HELP_ALLOW_READ_CALL_LOG">Controls retrieval of your list of recent incoming and outgoing calls.</string>
	<string name="SETTING_LABEL_ALLOW_READ_CALENDAR">Calendar</string>
	<string name="SETTING_HELP_ALLOW_READ_CALENDAR">Controls access to your calendar entries.</string>
	<string name="SETTING_LABEL_ALLOW_READ_SMS">Access SMS</string>
	<string name="SETTING_HELP_ALLOW_READ_SMS">Controls reading of text messages.</string>
	<string name="SETTING_LABEL_ALLOW_SEND_SMS">Send SMS</string>
	<string name="SETTING_HELP_ALLOW_SEND_SMS">Controls sending of text messages.</string>
	<string name="SETTING_LABEL_ALLOW_READ_MMS">Access MMS</string>
	<string name="SETTING_HELP_ALLOW_READ_MMS">Controls reading of multimedia messages.</string>
	<string name="SETTING_LABEL_ALLOW_SEND_MMS">Send MMS</string>
	<string name="SETTING_HELP_ALLOW_SEND_MMS">Controls sending of multimedia messages.</string>
	<string name="SETTING_LABEL_ALLOW_RECORD_AUDIO">Record Audio</string>
	<string name="SETTING_HELP_ALLOW_RECORD_AUDIO">Controls recording of audio at any time (not during phone calls).</string>
	<string name="SETTING_LABEL_ALLOW_CAMERA">Camera</string>
	<string name="SETTING_HELP_ALLOW_CAMERA">Controls taking of still photographs using the cameras on the device.</string>
	<string name="SETTING_LABEL_ALLOW_READ_BOOKMARKS_AND_HISTORY">Bookmarks and History</string>
	<string name="SETTING_HELP_ALLOW_READ_BOOKMARKS_AND_HISTORY">Controls retrieving of browser bookmarks and history.</string>
	<string name="SETTING_LABEL_ALLOW_READ_SYSTEM_LOGS">System Logs</string>
	<string name="SETTING_HELP_ALLOW_READ_SYSTEM_LOGS">Controls reading of system logs from the device, which may contain personal data. This does not protect against apps with root-level access reading your system logs.</string>
	<string name="SETTING_LABEL_ALLOW_RECEIVE_BOOT_COMPLETED_INTENT">Start on Boot</string>
	<string name="SETTING_HELP_ALLOW_RECEIVE_BOOT_COMPLETED_INTENT">Controls whether an app is notified of boot completion, which can be used to load the app on device start-up, or perform other activities.</string>
	<string name="SETTING_LABEL_ALLOW_READ_WIFI_INFO">Wifi Info</string>
	<string name="SETTING_HELP_ALLOW_READ_WIFI_INFO">Controls whether the app can retrieve a list of all configured wireless networks on the device, the current wifi network, nearby wifi access points, and current IP address (if configured by DHCP).</string>
	<string name="SETTING_LABEL_ALLOW_CHANGE_WIFI_STATE">Switch Wifi State</string>
	<string name="SETTING_HELP_ALLOW_CHANGE_WIFI_STATE">Controls whether the app can enable or disable wireless networking.</string>
	<string name="SETTING_LABEL_ALLOW_CHANGE_MOBILE_DATA_STATE">Switch Network State</string>
	<string name="SETTING_HELP_ALLOW_CHANGE_MOBILE_DATA_STATE">Controls whether the app can enable or disable mobile data.</string>
	<string name="SETTING_LABEL_ALLOW_READ_NETWORK_INFO">Network Info</string>
	<string name="SETTING_HELP_ALLOW_READ_NETWORK_INFO">Controls access to detailed information about the current network including connection state, roaming, DNS servers, IP Addresses, etc.</string>
	<string name="SETTING_LABEL_ALLOW_READ_ANDROID_ID">Android ID</string>
	<string name="SETTING_HELP_ALLOW_READ_ANDROID_ID">Controls reading of the unique Android installation ID, which is constant for the lifetime of the OS installation (including across backup and restore of the OS).</string>
	<string name="SETTING_LABEL_ALLOW_READ_SIM_INFO">SIM Info</string>
	<string name="SETTING_HELP_ALLOW_READ_SIM_INFO">Controls reading of the SIM country and operator.</string>
	<string name="SETTING_LABEL_ALLOW_ACCESS_IP_TABLES">IP Tables</string>
	<string name="SETTING_HELP_ALLOW_ACCESS_IP_TABLES">Limits app ability to execute scripts which include iptables commands (or more accurately, contains the text iptables or ip6tables)</string>
	<string name="SETTING_LABEL_ALLOW_READ_ICC_ID">ICC Access</string>
	<string name="SETTING_HELP_ALLOW_READ_ICC_ID">Controls access to text messages on the SIM or R-UIM.</string>
	<string name="SETTING_LABEL_FAKE_ALWAYS_ONLINE">Force Online State</string>
	<string name="SETTING_HELP_FAKE_ALWAYS_ONLINE">If Yes (checked), the application will always be told that it a network connection is available.</string>
	
	<string name="SETTING_GROUP_LABEL_GENERAL">General</string>
	<string name="SETTING_GROUP_LABEL_PHONE">Phone &amp; Calls</string>
	<string name="SETTING_GROUP_LABEL_MESSAGES">Messages</string>
	<string name="SETTING_GROUP_LABEL_PERSONAL_DATA">Personal Data</string>
	<string name="SETTING_GROUP_LABEL_MEDIA">Media</string>
	<string name="SETTING_GROUP_LABEL_NETWORK">Network</string>
	<string name="SETTING_GROUP_LABEL_LOCATION">Location</string>
</resources>